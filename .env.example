DATABASE_HOST=127.0.0.1
DATABASE_PORT=3306
DATABASE_USERNAME=root
DATABASE_PASSWORD=
DATABASE_NAME=server
JWT_SECRET=your_jwt_secret_here
PORT=3000

# CORS Configuration
# Comma-separated list of allowed origins (empty for all origins)
CORS_ORIGINS=http://localhost:3000,https://your-domain.com
# Enable credentials (cookies, authorization headers)
CORS_CREDENTIALS=true

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Redis Configuration
# Redis Mode Configuration (single/cluster)
REDIS_MODE=single

# Single Node Redis Configuration (only needed if REDIS_MODE=single)
REDIS_HOST=localhost
REDIS_PORT=6379

# Cluster Mode Redis Configuration (only needed if REDIS_MODE=cluster)
REDIS_CLUSTER_NODES=localhost:6379,localhost:6380,localhost:6381

# Redis Common Configuration
REDIS_PASSWORD=

# S3 Configuration
S3_ACCESS_KEY_ID=your_access_key_id
S3_SECRET_ACCESS_KEY=your_secret_access_key
S3_REGION=your_region
S3_BUCKET=your_bucket_name
S3_ENDPOINT=your_s3_endpoint
S3_PUBLIC_URL=your_public_url
S3_PUBLIC=true
S3_BASE_PATH=ollie2024
# Set to true in production environment
S3_PRODUCTION=false